package edu;

public class Testing {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int dan = 3;
		

		for (int a = 2; a <= 9 ; a += dan) {
			for (int b = 2; b <= 9 ; b++) {
				for(int c = 0; c < dan ;c++ ) {
					System.out.printf("%d * %d = %d \t", b , a , a*b);
					
					if (c < dan -1) System.out.println("\t");
					else			System.out.println("\n");
				}
			}			
			System.out.println("");
		}
		
	}

}
